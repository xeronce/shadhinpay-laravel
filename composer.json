{
    "name": "xeronce/shadhinpay",
    "description": "Shadhinpay payment package for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tazim Hossain",
            "email": "tazim@xeronce.com"
        }
